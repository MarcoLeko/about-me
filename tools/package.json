{
  "name": "help-educate-tools",
  "version": "1.0.0",
  "description": "Contains useful scripts",
  "main": "index.js",
  "scripts": {
    "insert:db": "node -r dotenv/config ./scripts/db/insert-documents.js dotenv_config_path=../.env",
    "generate:statistics": "node -r dotenv/config ./scripts/db/map-statistics-generator.js dotenv_config_path=../.env",
    "clean:up": "node ./scripts/db/clean-up-files.js",
    "pack:bundle": "node ./scripts/aws/pack-bundle.js",
    "deploy": "cd ../backend && npm run build:prod && cd ../tools && npm run pack:bundle && node ./scripts/aws/modify-deploy-script.js && cd .. && eb deploy --profile eb-cli"
  },
  "author": "Marco Leko",
  "license": "ISC",
  "dependencies": {
    "archiver": "^3.1.1",
    "chalk": "^2.4.2",
    "dotenv": "^8.2.0",
    "mongodb": "^3.3.3",
    "node-fetch": "^2.6.0"
  }
}
